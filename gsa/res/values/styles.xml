<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="GovScreenTitleText" parent="PrimaryText">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:shadowColor">#DADADA</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="GovScreenMsgText">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:shadowColor">#DADADA</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:textStyle">normal</item>
    </style>
    
    <color name="ErrorTextColor">#E81E25</color>
        
	<style name="RefundableText">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#0022BA</item>
    </style>

</resources>