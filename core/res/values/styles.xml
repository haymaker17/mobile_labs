    <style name="ConcurBaseTheme" parent="Theme.AppCompat.Light.DarkActionBar">

        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="actionBarStyle">@style/Widget.Styled.ActionBar</item>
        <item name="actionOverflowButtonStyle">@style/ActionBarOverflowButton</item>
    </style>

    <style name="ConcurBaseTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">

        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ConcurBaseTheme.Translucent">
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="ConcurBaseTheme.Translucent.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ConcurTheme" parent="ConcurBaseTheme">
        <item name="android:textAppearanceSmall">@style/PrimaryText</item>
    </style>

    <style name="ConcurTheme.Dialog" parent="android:Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <!--
             Following parameter is needed for AppCompat v22.+
             and relevant only if this style is applied to an
             activity which inherits from AppCompatActivity.
         -->
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ConcurTheme.Dialog.Light" parent="android:Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <!--
             Following parameter is needed for AppCompat v22.+
             and relevant only if this style is applied to an
             activity which inherits from AppCompatActivity.
         -->
        <item name="windowNoTitle">true</item>
    </style>

    <!-- MOB-14596 - work around to fix in 2.x devices -->
    <style name="ConcurTheme.Sherlock.Dialog.Light" parent="Theme.AppCompat.Dialog">
        <item name="android:windowNoTitle">true</item>
        <!--
             Following parameter is needed for AppCompat v22.+
             and relevant only if this style is applied to an
