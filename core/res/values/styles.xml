    <!-- New Login Screen -->
    <style name="ConcurBaseTheme.New" parent="Theme.AppCompat.Light">

        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:actionBarStyle">@style/Widget.Styled.ActionBar.New</item>
        <item name="actionMenuTextColor">@color/universal_tour_ab_menu</item>
        <item name="homeAsUpIndicator">@drawable/icon_drawer</item>
        <item name="actionOverflowButtonStyle">@style/ActionBarOverflowButton</item>
    </style>

    <style name="ConcurBaseTheme.White.New" parent="Theme.AppCompat.Light">
        <item name="actionBarStyle">@style/Widget.Styled.ActionBar.New</item>
        <item name="actionMenuTextColor">@color/universal_tour_ab_menu</item>
        <item name="homeAsUpIndicator">@drawable/icon_drawer</item>
        <item name="actionOverflowButtonStyle">@style/ActionBarOverflowButton.White</item>
    </style>

    <!-- White ActionBar with ActionBar tabs with blue text/bar -->

    <style name="ConcurBaseTheme.White.ActionBarTabs" parent="Theme.AppCompat.Light">
        <item name="actionOverflowButtonStyle">@style/ActionBarOverflowButton.Blue</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.White</item>
        <item name="actionBarTabTextStyle">@style/ActionBarTabTextStyle.Blue</item>
        <item name="actionBarStyle">@style/Concur.ActionBar.Tabs</item>
        <item name="actionMenuTextColor">@color/universal_tour_ab_menu</item>
        <item name="homeAsUpIndicator">@drawable/icon_drawer</item>
        <item name="popupMenuStyle">@style/ActionBarOverflowPopupWindow</item>
    </style>

    <style name="Concur.ActionBar.Tabs" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="android:background">@drawable/ab_tab_white_bg</item>
        <item name="backgroundStacked">@drawable/ab_tab_white_stacked_bg</item>
        <item name="backgroundSplit">@drawable/ab_tab_white_split_bg</item>
        <item name="displayOptions">showTitle</item>
        <item name="titleTextStyle">@style/NewActionBarNewHeaderText</item>
        <item name="actionMenuTextColor">@color/universal_tour_ab_menu</item>
    </style>

